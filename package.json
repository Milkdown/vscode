{
    "name": "milkdown-viewer",
    "private": true,
    "icon": "logo.png",
    "displayName": "Milkdown Viewer",
    "description": "Edit or view markdown in a WYSIWYG way, powered by milkdown.",
    "version": "0.0.20",
    "engines": {
        "vscode": "^1.74.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/yuya296/vscode-markdown.git"
    },
    "categories": [
        "Visualization",
        "Programming Languages"
    ],
    "activationEvents": [
        "onCustomEditor:milkdownViewer.editor"
    ],
    "keywords": [
        "markdown",
        "milkdown",
        "wysiwyg"
    ],
    "main": "./dist/extension.js",
    "publisher": "mirone",
    "contributes": {
        "customEditors": [
            {
                "viewType": "milkdownViewer.editor",
                "displayName": "Milkdown Viewer",
                "selector": [
                    {
                        "filenamePattern": "*.md"
                    }
                ],
                "priority": "option"
            }
        ],
        "keybindings": [
            {
                "key": "ctrl+b",
                "mac": "cmd+b",
                "command": "extension.milkdownViewer.bold",
                "when": "milkdownViewer.active && !inDebugRepl"
            },
            {
                "key": "ctrl+i",
                "mac": "cmd+i",
                "command": "extension.milkdownViewer.italic",
                "when": "milkdownViewer.active && !inDebugRepl"
            },
            {
                "key": "ctrl+e",
                "mac": "cmd+e",
                "command": "extension.milkdownViewer.inline_code",
                "when": "milkdownViewer.active && !inDebugRepl"
            },
            {
                "key": "ctrl+alt+x",
                "mac": "cmd+alt+x",
                "command": "extension.milkdownViewer.strike_through",
                "when": "milkdownViewer.active && !inDebugRepl"
            },
            {
                "key": "ctrl+alt+0",
                "mac": "cmd+alt+0",
                "command": "extension.milkdownViewer.text",
                "when": "milkdownViewer.active && !inDebugRepl"
            },
            {
                "key": "ctrl+alt+1",
                "mac": "cmd+alt+1",
                "command": "extension.milkdownViewer.h1",
                "when": "milkdownViewer.active && !inDebugRepl"
            },
            {
                "key": "ctrl+alt+2",
                "mac": "cmd+alt+2",
                "command": "extension.milkdownViewer.h2",
                "when": "milkdownViewer.active && !inDebugRepl"
            },
            {
                "key": "ctrl+alt+3",
                "mac": "cmd+alt+3",
                "command": "extension.milkdownViewer.h3",
                "when": "milkdownViewer.active && !inDebugRepl"
            },
            {
                "key": "ctrl+alt+4",
                "mac": "cmd+alt+4",
                "command": "extension.milkdownViewer.h4",
                "when": "milkdownViewer.active && !inDebugRepl"
            },
            {
                "key": "ctrl+alt+5",
                "mac": "cmd+alt+5",
                "command": "extension.milkdownViewer.h5",
                "when": "milkdownViewer.active && !inDebugRepl"
            },
            {
                "key": "ctrl+alt+6",
                "mac": "cmd+alt+6",
                "command": "extension.milkdownViewer.h6",
                "when": "milkdownViewer.active && !inDebugRepl"
            },
            {
                "key": "ctrl+alt+7",
                "mac": "cmd+alt+7",
                "command": "extension.milkdownViewer.ordered_list",
                "when": "milkdownViewer.active && !inDebugRepl"
            },
            {
                "key": "ctrl+alt+8",
                "mac": "cmd+alt+8",
                "command": "extension.milkdownViewer.bullet_list",
                "when": "milkdownViewer.active && !inDebugRepl"
            },
            {
                "key": "ctrl+alt+9",
                "mac": "cmd+alt+9",
                "command": "extension.milkdownViewer.task_list",
                "when": "milkdownViewer.active && !inDebugRepl"
            },
            {
                "key": "ctrl+[",
                "mac": "cmd+[",
                "command": "extension.milkdownViewer.lift",
                "when": "milkdownViewer.active && !inDebugRepl"
            },
            {
                "key": "ctrl+]",
                "mac": "cmd+]",
                "command": "extension.milkdownViewer.sink",
                "when": "milkdownViewer.active && !inDebugRepl"
            },
            {
                "key": "ctrl+enter",
                "mac": "cmd+enter",
                "command": "extension.milkdownViewer.exit_block",
                "when": "milkdownViewer.active && !inDebugRepl"
            },
            {
                "key": "shift+enter",
                "command": "extension.milkdownViewer.line_break",
                "when": "milkdownViewer.active && !inDebugRepl"
            },
            {
                "key": "ctrl+alt+c",
                "mac": "cmd+alt+c",
                "command": "extension.milkdownViewer.code",
                "when": "milkdownViewer.active && !inDebugRepl"
            },
            {
                "key": "tab",
                "mac": "tab",
                "command": "extension.milkdownViewer.tab",
                "when": "milkdownViewer.active && !inDebugRepl"
            },
            {
                "key": "ctrl+z",
                "mac": "cmd+z",
                "command": "extension.milkdownViewer.undo",
                "when": "milkdownViewer.active && !inDebugRepl"
            },
            {
                "key": "ctrl+shift+z",
                "mac": "cmd+shift+z",
                "command": "extension.milkdownViewer.redo",
                "when": "milkdownViewer.active && !inDebugRepl"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run package",
        "compile": "tsup",
        "package": "tsup",
        "watch": "tsup --watch src",
        "pretest": "npm run tsc && npm run lint",
        "lint:ox": "oxlint -c .oxlintrc.json",
        "lint:ox:fix": "oxlint -c .oxlintrc.json --fix",
        "lint:prettier": "prettier . --check",
        "lint:prettier:fix": "prettier . --write",
        "typecheck": "tsc --noEmit",
        "vscode-package": "vsce package --no-dependencies"
    },
    "devDependencies": {
        "@babel/runtime": "^7.26.7",
        "@milkdown/crepe": "^7.6.2",
        "@milkdown/kit": "^7.6.2",
        "@types/lodash.throttle": "^4.1.9",
        "@types/node": "~24.10.0",
        "@types/vscode": "^1.59.0",
        "@uiw/codemirror-theme-vscode": "^4.23.2",
        "@vscode/vsce": "^3.2.2",
        "concurrently": "^9.0.0",
        "esbuild": "^0.27.0",
        "glob": "^13.0.0",
        "lodash.throttle": "^4.1.1",
        "normalize.css": "^8.0.1",
        "oxlint": "^1.0.0",
        "prettier": "^3.0.0",
        "rimraf": "^6.0.1",
        "tsup": "^8.3.6",
        "typescript": "^5.6.2"
    },
    "packageManager": "pnpm@10.24.0"
}
